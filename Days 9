import math

def volume_kubus(sisi):
    return sisi ** 3

def luas_permukaan_kubus(sisi):
    return 6 * (sisi ** 2)

def volume_balok(panjang, lebar, tinggi):
    return panjang * lebar * tinggi

def luas_permukaan_balok(panjang, lebar, tinggi):
    return 2 * (panjang * lebar + panjang * tinggi + lebar * tinggi)

def volume_bola(jari_jari):
    return (4/3) * math.pi * (jari_jari ** 3)

def luas_permukaan_bola(jari_jari):
    return 4 * math.pi * (jari_jari ** 2)

def volume_tabung(jari_jari, tinggi):
    return math.pi * (jari_jari ** 2) * tinggi

def luas_permukaan_tabung(jari_jari, tinggi):
    return 2 * math.pi * jari_jari * (jari_jari + tinggi)

def volume_kerucut(jari_jari, tinggi):
    return (1/3) * math.pi * (jari_jari ** 2) * tinggi

def luas_permukaan_kerucut(jari_jari, tinggi):
    selimut = math.sqrt((jari_jari ** 2) + (tinggi ** 2))
    return math.pi * jari_jari * (jari_jari + selimut)

print("Volume Kubus:", volume_kubus(3))
print("Luas Permukaan Kubus:", luas_permukaan_kubus(3))
print("Volume Balok:", volume_balok(3, 4, 5))
print("Luas Permukaan Balok:", luas_permukaan_balok(3, 4, 5))
print("Volume Bola:", volume_bola(3))
print("Luas Permukaan Bola:", luas_permukaan_bola(3))
print("Volume Tabung:", volume_tabung(3, 5))
print("Luas Permukaan Tabung:", luas_permukaan_tabung(3, 5))
print("Volume Kerucut:", volume_kerucut(3, 5))
print("Luas Permukaan Kerucut:", luas_permukaan_kerucut(3, 5))
